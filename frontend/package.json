{
  "name": "mitt-telenor-demo",
  "version": "1.0.0",
  "description": "Telenor Self-Service Platform - Progressive Web App",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest --coverage",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:hard-skills": "playwright test --grep '1. HARD SKILLS'",
    "test:auth": "playwright test --grep '2. AUTHENTICATION'",
    "test:functionality": "playwright test --grep '3. CORE FUNCTIONALITY'",
    "test:performance": "playwright test --grep '4. PERFORMANCE'",
    "test:accessibility": "playwright test --grep '5. ACCESSIBILITY'",
    "test:pwa": "playwright test --grep '6. PWA'",
    "test:security": "playwright test --grep '7. SECURITY'",
    "test:responsive": "playwright test --grep '8. RESPONSIVE'",
    "test:navigation": "playwright test --grep '9. NAVIGATION'",
    "test:report": "playwright show-report",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.5.1",
    "@capacitor/device": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/share": "^5.0.6",
    "@capacitor/storage": "^1.2.5",
    "@supabase/supabase-js": "^2.91.0",
    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "contentful": "^10.6.21",
    "date-fns": "^3.2.0",
    "lucide-vue-next": "^0.309.0",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.4",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.5",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  }
}
